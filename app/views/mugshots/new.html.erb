	try{
		if ($.browser.mozilla && Math.floor(window.mozInnerScreenX) != window.mozInnerScreenX ) {
			$('#full_upload').css('margin-left' , '0.5px');
		}
	}catch(ex){}
	var flashvars = {};
	var params = {userid:<%= @current_authuser.id %>, auth:0, pathurl: "/swf/paths.xml"};
	var attributes = {};
	swfobject.embedSWF("http://<%= request.host %>/swf/fullupload.swf", "fullupload", "480", "540", "9.0.115", flashvars, params, attributes);
	</script>
</div>

